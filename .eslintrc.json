{
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "import",
    "jest"
  ],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "comma-dangle": "off",
    "padded-blocks": [
      "error",
      {
        "classes": "always"
      }
    ]
  }
}
